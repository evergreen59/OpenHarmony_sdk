import("//build/test.gni")

ohos_unittest("libtoken_callback_test") {
  subsystem_name = "security"
  part_name = "access_token"
  module_out_path = part_name + "/" + part_name

  include_dirs = [
    "//commonlibrary/c_utils/base/include",
    "//third_party/googletest/include",
    "//base/security/access_token/interfaces/innerkits/token_callback/include",
  ]

  sources = [ "unittest/src/token_callback_test.cpp" ]

  cflags_cc = [ "-DHILOG_ENABLE" ]

  configs = [ "//base/security/access_token/config:coverage_flags" ]

  deps = [ "//base/security/access_token/interfaces/innerkits/token_callback:libtoken_callback_sdk" ]
  external_deps = [
    "c_utils:utils",
    "ipc:ipc_core",
  ]
}

group("unittest") {
  testonly = true
  deps = [ ":libtoken_callback_test" ]
}
