genfscon sysfs /fs/hmdfs          u:object_r:sys_fs_hmdfs:s0
genfscon vfat /  u:object_r:vfat:s0
genfscon exfat /  u:object_r:exfat:s0
genfscon fuseblk /  u:object_r:ntfs:s0
