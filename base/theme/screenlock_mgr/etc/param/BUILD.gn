import("//build/ohos.gni")

#################################################################################

group("screenlock_param_group") {
  deps = [
    ":screenlock.para",
    ":screenlock.para.dac",
  ]
}

ohos_prebuilt_etc("screenlock.para") {
  source = "screenlock.para"
  relative_install_dir = "param"
  subsystem_name = "theme"
  part_name = "screenlock_mgr"
}

ohos_prebuilt_etc("screenlock.para.dac") {
  source = "screenlock.para.dac"
  relative_install_dir = "param"
  subsystem_name = "theme"
  part_name = "screenlock_mgr"
}
