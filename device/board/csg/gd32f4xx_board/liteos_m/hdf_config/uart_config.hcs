            rxPin = 9;         // UART_rx管脚  0~15: GPIO_PIN_0 ~ GPIO_PIN_15
            rx_AF = 8;         // GPIO 备用功能: 0~15: GPIO_AF_0 ~ GPIO_AF_15
            rx_DMA = 1;        // DMA: 0:DMA0  1:DMA1
            rx_DmaCh = 1;      // DMA通道选择: 0~7: DMA_CH0 ~ DMA_CH7
            rx_DmaPerSel = 5;  // DMA外设选择  0~7: DMA_SUBPERI0 ~ DMA_SUBPERI7
            txPort = 6;
            txPin = 14;
            tx_AF = 8;
            tx_DMA = 1;
            tx_DmaCh = 7;
            tx_DmaPerSel = 5;
        }
        uart6_config :: uart_config {
            match_attr = "gd32f470_uart_6";
            //uart config
            num = 6;          // UART6
            enable = 1;
