root {
    platform {
        template uart_config {
            match_attr = "";
            baudval = 115200;
        }
        uart0_config :: uart_config {
            match_attr = "gd32f470_uart_0";
            //uart config
            num = 0;          //USART0
            enable = 0;
            rxPort = 0;      // UART_rx管脚组 0:GPIOA 1:GPIOB 2:GPIOC 3:GPIOD 4:GPIOE 5: GPIOF 6:GPIOG 7:GPIOH 8:GPIOI
            rxPin = 0;
            rx_AF = 0;
            txPort = 0;
            txPin = 0;
            tx_AF = 0;
            rx_DMA = 0;
            rx_DmaCh = 0;
            rx_DmaPerSel = 0;
            tx_DMA = 0;
            tx_DmaCh = 0;
            tx_DmaPerSel = 0;
        }
        uart1_config :: uart_config {
            match_attr = "gd32f470_uart_1";
            //uart config
            num = 1;          // USART1
            enable = 1;
            rxPort = 0;        // UART_rx管脚组 0:GPIOA 1:GPIOB 2:GPIOC 3:GPIOD 4:GPIOE 5: GPIOF 6:GPIOG 7:GPIOH 8:GPIOI
            rxPin = 3;         // UART_rx管脚  0~15: GPIO_PIN_0 ~ GPIO_PIN_15
            rx_AF = 7;         // GPIO 备用功能: 0~15: GPIO_AF_0 ~ GPIO_AF_15
            rx_DMA = 0;        // DMA: 0:DMA0  1:DMA1
            rx_DmaCh = 5;      // DMA通道选择: 0~7: DMA_CH0 ~ DMA_CH7
            rx_DmaPerSel = 4;  // DMA外设选择  0~7: DMA_SUBPERI0 ~ DMA_SUBPERI7
            txPort = 0;
            txPin = 2;
            tx_AF = 7;
            tx_DMA = 0;
            tx_DmaCh = 6;
            tx_DmaPerSel = 4;
        }
        uart2_config :: uart_config {
            match_attr = "gd32f470_uart_2";
            //uart config
            num = 2;          // USART2
            enable = 1;
            rxPort = 1;        // UART_rx管脚组 0:GPIOA 1:GPIOB 2:GPIOC 3:GPIOD 4:GPIOE 5: GPIOF 6:GPIOG 7:GPIOH 8:GPIOI
            rxPin = 11;         // UART_rx管脚  0~15: GPIO_PIN_0 ~ GPIO_PIN_15
            rx_AF = 7;         // GPIO 备用功能: 0~15: GPIO_AF_0 ~ GPIO_AF_15
            rx_DMA = 0;        // DMA: 0:DMA0  1:DMA1
            rx_DmaCh = 1;      // DMA通道选择: 0~7: DMA_CH0 ~ DMA_CH7
            rx_DmaPerSel = 4;  // DMA外设选择  0~7: DMA_SUBPERI0 ~ DMA_SUBPERI7
            txPort = 1;
            txPin = 10;
            tx_AF = 7;
            tx_DMA = 0;
            tx_DmaCh = 3;
            tx_DmaPerSel = 4;
        }
