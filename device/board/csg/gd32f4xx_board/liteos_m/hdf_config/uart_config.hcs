            match_attr = "gd32f470_uart_3";
            //uart config
            num = 3;          // USART2
            enable = 1;
            rxPort = 2;        // UART_rx管脚组 0:GPIOA 1:GPIOB 2:GPIOC 3:GPIOD 4:GPIOE 5: GPIOF 6:GPIOG 7:GPIOH 8:GPIOI
            rxPin = 11;         // UART_rx管脚  0~15: GPIO_PIN_0 ~ GPIO_PIN_15
            rx_AF = 8;         // GPIO 备用功能: 0~15: GPIO_AF_0 ~ GPIO_AF_15
            rx_DMA = 0;        // DMA: 0:DMA0  1:DMA1
            rx_DmaCh = 2;      // DMA通道选择: 0~7: DMA_CH0 ~ DMA_CH7
            rx_DmaPerSel = 4;  // DMA外设选择  0~7: DMA_SUBPERI0 ~ DMA_SUBPERI7
            txPort = 2;
            txPin = 10;
            tx_AF = 8;
            tx_DMA = 0;
            tx_DmaCh = 4;
            tx_DmaPerSel = 4;
        }
        uart4_config :: uart_config {
            match_attr = "gd32f470_uart_4";
