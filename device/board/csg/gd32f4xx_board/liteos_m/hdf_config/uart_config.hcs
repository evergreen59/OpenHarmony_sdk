            rxPin = 2;         // UART_rx管脚  0~15: GPIO_PIN_0 ~ GPIO_PIN_15
            rx_AF = 8;         // GPIO 备用功能: 0~15: GPIO_AF_0 ~ GPIO_AF_15
            rx_DMA = 0;        // DMA: 0:DMA0  1:DMA1
            rx_DmaCh = 0;      // DMA通道选择: 0~7: DMA_CH0 ~ DMA_CH7
            rx_DmaPerSel = 4;  // DMA外设选择  0~7: DMA_SUBPERI0 ~ DMA_SUBPERI7
