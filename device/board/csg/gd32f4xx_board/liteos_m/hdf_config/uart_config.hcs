            rxPin = 7;         // UART_rx管脚  0~15: GPIO_PIN_0 ~ GPIO_PIN_15
            rx_AF = 8;         // GPIO 备用功能: 0~15: GPIO_AF_0 ~ GPIO_AF_15
            rx_DMA = 0;        // DMA: 0:DMA0  1:DMA1
            rx_DmaCh = 3;      // DMA通道选择: 0~7: DMA_CH0 ~ DMA_CH7
            rx_DmaPerSel = 5;  // DMA外设选择  0~7: DMA_SUBPERI0 ~ DMA_SUBPERI7
            txPort = 4;
            txPin = 8;
            tx_AF = 8;
            tx_DMA = 0;
            tx_DmaCh = 1;
            tx_DmaPerSel = 5;
        }
        uart7_config :: uart_config {
            match_attr = "gd32f470_uart_7";
            //uart config
            num = 7;          // UART7
            enable = 0;
            rxPort = 4;        // UART_rx管脚组 0:GPIOA 1:GPIOB 2:GPIOC 3:GPIOD 4:GPIOE 5: GPIOF 6:GPIOG 7:GPIOH 8:GPIOI
            rxPin = 0;         // UART_rx管脚  0~15: GPIO_PIN_0 ~ GPIO_PIN_15
            rx_AF = 8;         // GPIO 备用功能: 0~15: GPIO_AF_0 ~ GPIO_AF_15
            rx_DMA = 0;        // DMA: 0:DMA0  1:DMA1
            rx_DmaCh = 6;      // DMA通道选择: 0~7: DMA_CH0 ~ DMA_CH7
            rx_DmaPerSel = 5;  // DMA外设选择  0~7: DMA_SUBPERI0 ~ DMA_SUBPERI7
            txPort = 4;
            txPin = 1;
            tx_AF = 8;
            tx_DMA = 0;
            tx_DmaCh = 0;
            tx_DmaPerSel = 5;
        }
    }
}
