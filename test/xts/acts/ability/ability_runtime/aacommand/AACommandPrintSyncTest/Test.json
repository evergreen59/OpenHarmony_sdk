{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "600000",
        "bundle-name": "com.example.aacommandprintsynctest",
        "module-name": "entry_test",
        "shell-timeout": "600000"
    },
    "kits": [
        {
            "type": "ShellKit"
        },
        {
            "type": "PushKit",
            "push": [
                "AACommandPrintSync.hap->/data/AACommandPrintSync.hap"
            ]
        },
		{
            "test-file-name": [
                "ActsAACommandPrintSyncTest.hap",
                "AACommandPrintSync.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        }
    ]
}

