        "icon": "$media:icon",
        "label": "$string:MainAbility4_label"
      },
      {
        "name": "MainAbility5",
        "srcEntrance": "./ets/MainAbility5/MainAbility5.ts",
        "description": "$string:MainAbility5_desc",
        "icon": "$media:icon",
        "label": "$string:MainAbility5_label"
