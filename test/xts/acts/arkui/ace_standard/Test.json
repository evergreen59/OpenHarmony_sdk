{
    "description": "Configuration for aceceshi Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "2500000",
        "package": "com.example.aceceshi",
        "shell-timeout": "1800000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActsAceStandardTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
		{
            "type": "ShellKit",
            "run-command": [
                "hilog -Q pidoff"
            ]
        }
    ]
}
