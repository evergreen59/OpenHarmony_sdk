{
    "description": "Configuration for geolocation js api Tests",
    "driver": {
        "type": "JSUnitTest",
        "test-timeout": "600000",
        "package": "ohos.acts.location.geolocation.function",
        "shell-timeout": "60000"
    },
    "kits": [
        {
            "test-file-name": [
                "ActslocationErrorCodeApiTest.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
		{
            "type": "ShellKit",
            "run-command": [
                "hilog -Q pidoff",
                "settings put SECURE location_mode 3"
            ]
        }
    ]
}
