{
    "description": "Function test of sim manager interface",
    "driver": {
        "type": "OHJSUnitTest",
        "test-timeout": "1800000",
        "bundle-name": "com.ohos.register",
        "package-name": "com.ohos.register",
        "shell-timeout": "1800000",
        "testcase-timeout": "30000"
    },
    "kits": [
        {
            "test-file-name": [
                "$module.hap"
            ],
            "type": "AppInstallKit",
            "cleanup-apps": true
        },
        {
            "type": "ShellKit",
            "run-command": [
                "power-shell wakeup",
                "power-shell setmode 602",
                "uinput -T -m 425 1000 425 500 400"
            ]
        }
    ]
}
