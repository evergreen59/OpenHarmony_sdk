        {
            "pre-push": [
             "mount -o rw,remount /",
             "rm /data/test/*",
             "mkdir -p /data/test"
            ],
            "push": [
                "HatsHdfAudioRenderControlTest->/data/local/tmp/HatsHdfAudioRenderControlTest",
                "./resource/audio/audiorendertest.wav->/data/test",
                "./resource/audio/lowlatencyrendertest.wav->/data/test"
            ],
            "type": "PushKit"
        }
    ],
    "driver": {
        "native-test-timeout": "120000",
        "type": "CppTest",
        "module-name": "HatsHdfAudioRenderControlTest",
        "runtime-hint": "1s",
        "native-test-device-path": "/data/local/tmp"
    },
    "description": "Configuration for HatsHdfAudioRenderControlTest Tests"
}