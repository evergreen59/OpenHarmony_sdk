        {
          "component": "init_lite",
          "features": [
            "enable_ohos_startup_init_lite_use_posix_file_api = true",
            "enable_ohos_startup_init_feature_begetctl_liteos = true",
            "config_ohos_startup_init_lite_data_path = \"/data/\""
